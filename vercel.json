{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    },
    "api/swagger.ts": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api",
      "dest": "/api/swagger.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/swagger.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
